{
    "version": 2,
    "name": "cow-engine",
    "builds": [
      { "src": "server.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/server.js" }
    ],
    "crons": [
      {
        "path": "/job?key=dairy-farm-job",
        "schedule": "00 20 * * *"
      }
    ]
  }
